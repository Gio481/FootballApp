        </attr>
        <attr name="teamType">
            <enum name="home" value="0" />
            <enum name="away" value="1" />
        </attr>
        <attr name="action" format="string" />
        <attr name="mainActionPlayer" format="string" />
        <attr name="subOnPlayer" format="string" />
        <attr name="subOffPlayer" format="string" />
    </declare-styleable>
</resources>