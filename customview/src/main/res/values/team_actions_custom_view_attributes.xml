        <attr name="actionType">
            <enum name="goal" value="0" />
            <enum name="yellowCard" value="1" />
            <enum name="redCard" value="2" />
            <enum name="substitution" value="3" />
        </attr>
        <attr name="goalType">
            <enum name="goal" value="0" />
